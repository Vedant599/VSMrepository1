<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Moodle">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.Moodle.ChangePassword"></activity>
        <activity android:name="com.example.Moodle.UpdateUserInfo" />
        <activity android:name="com.example.Moodle.OTPVerification" />
        <activity android:name="com.example.Moodle.ProfileActivity" />
        <activity android:name="com.example.Moodle.ForgotPassword" />
        <activity android:name="com.example.Moodle.Register_Organiser" />
        <activity android:name="com.example.Moodle.Select_User" />
        <activity android:name="com.example.Moodle.Register_Student" />
        <activity android:name="com.example.Moodle.HomePage" />
        <activity android:name="com.example.Moodle.Splash_screen_activity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.Moodle.MainActivity" />
    </application>

</manifest>